<div class="row">
  {{#each items}}
		<div class="col-md-6">
			<div class="well">
				<a class="link" href="{{url}}" target="_blank">
		      <h3>{{title}}</h3>
				</a>
				<span class="label label-default">
					{{year}}
				</span>
				<p>{{description}}</p>
			</div>
		</div>
  {{/each}}
</div>
